{
  "name": "gitpod-website",
  "description": "Gitpod.io Website",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "main": "n/a",
  "scripts": {
    "build": "cp -r src/docs/release-notes/* static/release-notes/ && gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "babel-plugin-styled-components": "1.10.7",
    "bowser": "2.8.1",
    "classnames": "2.2.6",
    "env-cmd": "^10.0.1",
    "gatsby": "2.19.7",
    "gatsby-image": "2.2.39",
    "gatsby-plugin-algolia-docsearch": "^1.0.5",
    "gatsby-plugin-canonical-urls": "2.1.19",
    "gatsby-plugin-catch-links": "2.1.24",
    "gatsby-plugin-emotion": "4.1.21",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-google-analytics": "2.1.34",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.3",
    "gatsby-plugin-sitemap": "2.2.26",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-remark-autolink-headers": "2.1.23",
    "gatsby-remark-copy-linked-files": "2.1.36",
    "gatsby-remark-embed-video": "^2..1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "3.1.42",
    "gatsby-remark-prismjs": "3.3.30",
    "gatsby-remark-responsive-iframe": "2.2.31",
    "gatsby-remark-smartypants": "2.1.20",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-json": "2.2.25",
    "gatsby-transformer-remark": "2.6.48",
    "gatsby-transformer-sharp": "2.3.13",
    "polished": "3.4.3",
    "prism-themes": "1.3.0",
    "prismjs": "1.19.0",
    "react": "16.12.0",
    "react-cookie-consent": "^3.0.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-modal-video": "1.2.3",
    "react-tweet-embed": "1.2.2",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/node": "10.14.10",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "gh-pages": "2.0.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "tslint": "6.0.0",
    "tslint-config-blvd": "1.2.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0"
  }
}
